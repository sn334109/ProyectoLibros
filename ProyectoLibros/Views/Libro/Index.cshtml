@using Dominio
@model List<Libro>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ISBN</th>
            <th scope="col">Nombre</th>
            <th scope="col">Autor</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Libro unLibro in Model)
        {
            <tr>
                <td>@unLibro.Isbn</td>
                <td>@unLibro.Nombre</td>
                <td>@unLibro.Autor</td>
            </tr>
        }
    </tbody>
</table>